programa
{
	
	funcao inicio() {

		inteiro idade, anoAtual, anoNas

		escreva("Digite o ano atual e seu ano de nacimento: ")
		leia(anoAtual, anoNas)

		idade = anoAtual - anoNas

		escreva("O usuario possui: ", idade, " anos.\n\n")
	}
}
